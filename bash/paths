# Add locally installed executables
PATH=$PATH:$HOME/bin
export PATH

# Misc paths
PATH=$PATH:/opt/redis:/opt/scala/bin
export PATH

# Sun Java (i.e. not OpenJDK)
JAVA_HOME=/usr/lib/jvm/java-6-sun/
export JAVA_HOME

# Add/remove as appropriate
source ~/.bash/path_scripts/clojure
source ~/.bash/path_scripts/nodejs
source ~/.bash/path_scripts/virtualenvwrapper
source ~/.bash/path_scripts/rbenv
